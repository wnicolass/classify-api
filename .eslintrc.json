{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/no-named-default": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
